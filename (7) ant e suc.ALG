Algoritmo "numeros"

Var
// Seção de Declarações das variáveis 
n1: Real
ant, suc:Real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
escreval("Qual o número?")
leia(n1)


suc <- n1 + 1
ant <- n1 - 1


escreval("O nº foi ", n1, " seu sucessor é ", suc, " e seu antecessor é ", ant)

Fimalgoritmo